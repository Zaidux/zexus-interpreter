entity Logger {
    name: string
    
    action log(message: string) {
        print(this.name + ": " + message)
        return true
    }
}

entity Calculator {
    name: string
    
    action add(a: integer, b: integer) -> integer {
        print(this.name + " computing...")
        return a + b
    }
}

let logger = Logger("MyLogger")
logger.log("Test message")

let calc = Calculator("MathBot")
let result = calc.add(5, 3)
print("Result: " + string(result))
