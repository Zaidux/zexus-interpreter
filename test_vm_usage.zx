// Test 1: Large program (>10 statements) - should trigger VM
print("=== Test 1: Large Program ===");
let a = 1;
let b = 2;
let c = 3;
let d = 4;
let e = 5;
let f = 6;
let g = 7;
let h = 8;
let i = 9;
let j = 10;
let k = 11;
