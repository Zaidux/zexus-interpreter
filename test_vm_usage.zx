// Test 2: While loop - should trigger VM
print("\n=== Test 2: While Loop ===");
let counter = 0;
while counter < 5 {
    counter = counter + 1;
}
print("Counter: " + string(counter));

// Test 3: For-each loop - should trigger VM
print("\n=== Test 3: For-Each Loop ===");
let sum = 0;
for each n in [1, 2, 3, 4, 5] {
    sum = sum + n;
}
print("Sum: " + string(sum));

// Test 4: Large function (>5 statements) - should trigger VM
print("\n=== Test 4: Large Function ===");
action complexFunction(x) {
    let result = x;
    result = result + 1;
    result = result * 2;
    result = result - 3;
    result = result / 2;
    result = result + 10;
    return result;
}
let funcResult = complexFunction(5);
print("Function result: " + string(funcResult));

print("\n=== All VM Tests Complete ===");
