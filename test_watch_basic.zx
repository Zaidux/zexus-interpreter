# Test WATCH keyword basic functionality
print "=== WATCH Keyword Test ===";

# Test 1: Implicit watch
let x = 10;
let y = 0;

watch {
    y = x * 2;
}

print "After implicit watch: y =", y;

# Test 2: Explicit watch
let a = 5;
let b = 0;

watch a => {
    b = a + 1;
}

print "After explicit watch setup: b =", b;

a = 10;
print "After changing a: b =", b;

print "âœ“ WATCH keyword works";
