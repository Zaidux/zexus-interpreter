entity Order {
    id: integer,
    status: string
}

action test1() -> string {
    let order = Order(1, "pending")
    order.status = "completed"
    persist_set("test1", order)  # Not capturing return value
    return "done"
}

action test2() -> string {
    let order = Order(2, "pending")
    order.status = "completed"
    let result = persist_set("test2", order)  # Capturing return value
    return "done"
}

print("Running test1 (no capture)...")
test1()

print("Running test2 (with capture)...")
test2()

print("\nChecking results:")
let r1 = persist_get("test1")
let r2 = persist_get("test2")

print("test1 result: " + string(r1))
print("test2 result: " + string(r2))
